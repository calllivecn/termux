
export LANG=zh_CN.UTF-8

if [ -d $HOME/bin ];then
	export PATH=$PATH:$HOME/bin
fi

# 设置termux hostname
if [ -r $PREFIX/etc/hostname ];then
	TERMUX_HOSTNAME=$(cat $PREFIX/etc/hostname)
else
	TERMUX_HOSTNAME="termux-autoboot"
fi

export TERMUX_HOSTNAME

# auto start tmux daemon
if [ -r $HOME/.autostart.sh ];then
	bash $HOME/.autostart.sh
fi
#########
#
